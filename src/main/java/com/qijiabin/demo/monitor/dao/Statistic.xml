<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE sqlMap      
    PUBLIC "-//ibatis.apache.org//DTD SQL Map 2.0//EN"      
    "http://ibatis.apache.org/dtd/sql-map-2.dtd">
<sqlMap namespace="Statistic">
  <typeAlias alias="Statistic" type="com.qijiabin.demo.monitor.bean.Statistic"/>
  
  <insert id="insertStatistic" parameterClass="Statistic">
    insert into statistic (service,method,time,concurrent,createTime,isError)
    	values (#service#, #method#, #time#, #concurrent#, #createTime#, #isError#)
  </insert>

</sqlMap>